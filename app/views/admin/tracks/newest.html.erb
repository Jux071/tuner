<div class = "row col-md-12">
	<div class = "well">
		<h3>Latest Tracks <%= link_to 'Add new track', new_admin_track_path, class: 'btn btn-sm btn-primary' %></h3>
		
	</div>		
	<div class="panel panel-default">  
  <div class="panel-heading"><strong>Track details</strong></div>
  	<table class="table table-hover table-striped">	
			<thead>
				<tr>
	    		<th>Name</th>
	    		<th>Duration</th>
	    		<th>Artist</th>
	    		<th>Genre</th>
	    		<th>Album</th>
	    		<th>Actions</th>
				</tr>
			</thead>

			<tbody>
				<%= render @tracks.order(name: :asc) %>
				
			</tbody>
		</table>
	</div>
</div>