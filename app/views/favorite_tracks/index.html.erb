<div class = "row col-md-12">
	<div class = "well">
		<h3>Favorite tracks <span class="badge"><%= @favorites %></h3>
	</div>		
	<div class="panel panel-default">  
  
  	<table class="table table-hover table-striped">	
			<thead>
				<tr>
	    		<th>Name</th>
	    		<th>Duration</th>
	    		<th>Artist</th>
	    		<th>Genre</th>
	    		<th>Album</th>
	    		<th>Actions</th>
				</tr>
			</thead>

			<tbody>
				<%= render 'track' %>
				
			</tbody>
		</table>
	</div>
</div>
